   "source": [
    "from scipy.stats import norm, expon # Note norm uses the mean and *standard deviation*, not variance!\n",
    "from numpy import sqrt, mean\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "\n",
    "ns = [1, 5, 10, 20, 50, 2000]\n",
    "fig, axs = plt.subplots(2, 3, figsize = (15, 10), sharex = True)\n",
    "fig.suptitle(\"AVERAGE of $X_i \\sim Expo(1)$\", fontsize = 25)\n",
    "for i,ax in enumerate(axs.flatten()):\n",
    "    X = expon(scale = 1).rvs((10000, ns[i]))  # Creates a 1000-by-10 matrix of draws from Z\n",
    "    Z = mean(X, axis = 1)  # Takes the mean of each row\n",
    "    ax.hist(Z, bins = 30, alpha = .5, normed = True)\n",
    "    ax.set_title(\"n = %d\" % ns[i], fontsize = 20)\n",
    "fig.savefig(\"lln\")"
   ]
  },
